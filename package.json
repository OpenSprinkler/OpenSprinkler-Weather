{
  "name": "os-weather-service",
  "description": "OpenSprinkler Weather Service",
  "version": "2.0.6",
  "repository": "https://github.com/OpenSprinkler/Weather-Weather",
  "scripts": {
    "test": "mocha --exit --require ts-node/register **/*.spec.ts",
    "start": "node js/server",
    "dev": "node scripts/server",
    "debug": "node --inspect=9229 js/server",
    "bundle": "npm run compile && zip weather.zip -r js package.json package-lock.json",
    "deploy": "npm run bundle && eb deploy",
    "compile": "tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "dotenv": "^16.1.4",
    "express": "^4.16.4",
    "geo-tz": "^7.0.7",
    "jsonwebtoken": "^9.0.0",
    "mockdate": "^3.0.5",
    "moment-timezone": "^0.5.25",
    "node-cache": "^5.1.2",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/cron": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^10.0.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^18.14.0",
    "@types/node-cache": "^4.1.3",
    "@types/suncalc": "^1.8.0",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "nock": "^13.3.1",
    "node-watch": "^0.7.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
